open_daily() {
    local today=$(date +%Y-%m-%d)
    local filename="${today}.org"

    if [[ -f "$filename" ]]; then
        echo "文件 $filename 已存在，正在打开..."
        emacs "$filename"
    else
        echo "文件 $filename 不存在，正在创建并打开..."
        cp 000-day-template.org "$filename"
        emacs "$filename"
    fi
}




